# Configuration file for the Sphinx documentation builder.
project = '@module_name@'
copyright = '@ARG_COPYRIGHT@'
author = '@ARG_AUTHORS@'
release = '@ARG_RELEASE@'
version = '@ARG_VERSION@'

extensions = [ 
    'breathe',
    'sphinx.ext.autodoc',
    'sphinx.ext.viewcode',
    'sphinx.ext.githubpages'
]

templates_path = ['_templates']
exclude_patterns = []
language = 'zh_CN'
html_theme = 'pydata_sphinx_theme'
html_static_path = ['_static']
breathe_default_project = "@module_name@"
